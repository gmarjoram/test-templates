{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Hacked github",
    "longDescription": "My hacked github schema",
    "type": "object",
    "properties": {
        "foo": {
            "type": "string"
        },
        "bar": {
            "type": "string",
            "default": "honk"
        },
        "baz": {
            "type": "string",
            "default": "blub"
        },
        "quux": {
            "type": "boolean",
            "default": true
        }
    },
    "required": [
        "foo",
        "bar"
    ],
    "form": [
        {
            "type": "validator",
            "url": "https://open-toolchain.github.io/test-templates/validator_assert/helper.html"
        },
        {
            "key": "foo"
        },
        {
            "key": "bar"
        },
        {
            "key": "baz"
        },
        {
            "key": "quux"
        }
    ]
}
